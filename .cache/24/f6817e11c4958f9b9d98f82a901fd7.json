\r\n        timeline.add({\r\n            targets: this,\r\n            scale: Card.defaultScale * 1.02,\r\n            duration: 150\r\n        });\r\n\r\n        const flipTo = Card.back == this.frame.name ? this.face : Card.back;\r\n\r\n        timeline.add({\r\n            targets: this,\r\n            scaleX: 0,\r\n            scaleY: 0.5,\r\n            duration: 200,\r\n            delay: 100,\r\n            onComplete: () => this.setTexture('cards', flipTo)\r\n        });\r\n\r\n        timeline.add({\r\n            targets: this,\r\n            scaleX: 1,\r\n            duration: 50\r\n        });\r\n\r\n        timeline.add({\r\n            targets: this,\r\n            scale: Card.defaultScale,\r\n            duration: 100\r\n        })\r\n\r\n        timeline.play();\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"22c27ce1a602c74afe535476715a8146","cacheData":{"env":{}}}